  {
    "name": "Hoth",
    "version": "0.1",
    "description": "Prototype twitter article checker",
    "permissions": ["activeTab", "storage"],
    "content_scripts": [
      {
        "matches": ["https://twitter.com/*"],
        "run_at": "document_start",
        "js": [
          "external/jquery-3.5.1.min.js",
          "hoth.js"
        ]
      }
    ],
    "manifest_version": 2
  }
